    "@geist-ui/react-icons": "^1.0.1",
    "@hookform/resolvers": "^2.6.0",
    "@types/cors": "^2.8.12",
    "@types/react-hamburger-menu": "^0.0.4",
    "autoprefixer": "^10.3.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.22.1",
    "dumb-passwords": "^0.2.1",
    "framer-motion": "^4.1.17",
    "human-date": "^1.4.0",
